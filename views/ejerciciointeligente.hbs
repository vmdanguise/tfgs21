<p>Gestion Registro</p>
<div id="datosTabla" style="width: 100%; height: 100px; overflow-y: scroll;">
        <table id="Table" border=1 bordercolor="white" width="100%">
                <thead>
                        <tr>
                                <td>idejercicioInteligente</td>
                                <td>tipo</td>
                                <td>escuela</td>
                                <td>asistenteVirtual</td>
                        </tr>
                </thead>
                <tbody id="contenido"> {{#each ejerciciointeligente}} <tr>
                                <td align="center">{{idejercicioInteligente}}</td>
                                <td align="center">{{tipo}}</td>
                                <td align="center">{{escuela}}</td>
                                <td align="center">{{asistenteVirtual}}</td>
                        </tr> {{/each}} </tbody>
        </table>
</div>
<form> <button type="submit" value="Imprimir" formaction="/ejerciciointeligente/imprimir_ejerciciointeligente"
                formmethod="POST"><img src="https://img.icons8.com/color/50/000000/print.png" /></button>
        <fieldset width="100"> <input type="hidden" id="token" size="500" maxlength="500" name="token"
                        value="{{token}}" />
                <legend>ABM</legend> <input class="btn" type="button" value="Buscar"
                        onclick="select_ejerciciointeligente()" /> <input class="btn" type="button" value="Alta"
                        onclick="insertar_ejerciciointeligente()" /> <input class="btn" type="button" value="Baja"
                        onclick="delete_ejerciciointeligente()" /> <input class="btn" type="button" value="Modificar"
                        onclick="update_ejerciciointeligente()" /> <input class="btn" type="button" value="Cancelar"
                        onclick="cancelar()" /> <input class="btn" type="button" value="Volver"
                        onclick="try{javascript:var mywindows = window.opener.document.getElementById('idejercicioInteligente').value = window.document.getElementById('idejercicioInteligente').value;}catch(error){};window.close()" />
                <br><br><br><br><label for="idejercicioInteligente">idejercicioInteligente</label><input type="number"
                        id="idejercicioInteligente" size="30" maxlength="30"
                        name="idejercicioInteligente" /><br><br><label for="tipo">tipo</label><input type="text"
                        id="tipo" size="30" maxlength="30" name="tipo" /><br><br><label
                        for="escuela">escuela</label><input type="text" id="escuela" size="30" maxlength="30"
                        name="escuela" /><br><br><label for="asistenteVirtual">asistenteVirtual</label><input
                        type="text" id="asistenteVirtual" size="30" maxlength="30" name="asistenteVirtual" /> <br><br>
                <button type="button" value="Buscar" onclick="select_ejerciciointeligente()"><img
                                src="https://img.icons8.com/color/48/000000/find-user-male--v1.png" alt="Buscar" />
                </button> <button type="button" value="Alta" onclick="insertar_ejerciciointeligente()"><img
                                src="https://img.icons8.com/color/50/000000/plus--v1.png" alt="Alta" /></button> <button
                        type="button" value="Baja" onclick="delete_ejerciciointeligente()"><img
                                src="https://img.icons8.com/nolan/50/delete.png" alt="Baja" /></button> <button
                        type="button" value="Modificar" onclick="update_ejerciciointeligente()"><img
                                src="https://img.icons8.com/nolan/50/edit--v1.png" alt="Modificar" /></button> <button
                        type="button" value="Cancelar" onclick="cancelar()"><img
                                src="https://img.icons8.com/officexs/50/000000/cancel-2.png" alt="Cancelar" /></button>
                <button type="button" value="Volver"
                        onclick="try{javascript:var mywindows = window.opener.document.getElementById('idejercicioInteligente').value = window.document.getElementById('idejercicioInteligente').value;}catch(error){};window.close()"><img
                                src="https://img.icons8.com/ios/50/000000/home.png" alt="Volver" /></button>
        </fieldset>
</form>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js"></script>
<script src="../../public/js/ejerciciointeligente_v.js"></script>
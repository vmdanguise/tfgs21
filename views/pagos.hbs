<p>Gestion Registro</p>
<div id="datosTabla" style="width: 100%; height: 100px; overflow-y: scroll;">
        <table id="Table" border=1 bordercolor="white" width="100%">
                <thead>
                        <tr>
                                <td>idpagos</td>
                                <td>fecha</td>
                                <td>idAlumno</td>
                        </tr>
                </thead>
                <tbody id="contenido"> {{#each pagos}} <tr>
                                <td align="center">{{idpagos}}</td>
                                <td align="center">{{fecha}}</td>
                                <td align="center">{{idAlumno}}</td>
                        </tr> {{/each}} </tbody>
        </table>
</div>
<form> <button type="submit" value="Imprimir" formaction="/pagos/imprimir_pagos" formmethod="POST"><img
                        src="https://img.icons8.com/color/50/000000/print.png" /></button>
        <fieldset width="100"> <input type="hidden" id="token" size="500" maxlength="500" name="token"
                        value="{{token}}" />
                <legend>ABM</legend> <input class="btn" type="button" value="Buscar" onclick="select_pagos()" /> <input
                        class="btn" type="button" value="Alta" onclick="insertar_pagos()" /> <input class="btn"
                        type="button" value="Baja" onclick="delete_pagos()" /> <input class="btn" type="button"
                        value="Modificar" onclick="update_pagos()" /> <input class="btn" type="button" value="Cancelar"
                        onclick="cancelar()" /> <input class="btn" type="button" value="Volver"
                        onclick="try{javascript:var mywindows = window.opener.document.getElementById('idpagos').value = window.document.getElementById('idpagos').value;}catch(error){};window.close()" />
                <br><br><br><br><label for="idpagos">idpagos</label><input type="number" id="idpagos" size="30"
                        maxlength="30" name="idpagos" /><br><br><label for="fecha">fecha</label><input type="date"
                        id="fecha" size="30" maxlength="30" name="fecha" /><br><br><label
                        for="idAlumno">idAlumno</label><input type="text" id="idAlumno" size="30" maxlength="30"
                        name="idAlumno" /><a href="javascript:var mywindows = window.open('./usuarios?token={{token}}')"><img
                                src="https://img.icons8.com/color/50/000000/search.png" alt=".(+)." /></a> <br><br>
                <button type="button" value="Buscar" onclick="select_pagos()"><img
                                src="https://img.icons8.com/color/48/000000/find-user-male--v1.png" alt="Buscar" />
                </button> <button type="button" value="Alta" onclick="insertar_pagos()"><img
                                src="https://img.icons8.com/color/50/000000/plus--v1.png" alt="Alta" /></button> <button
                        type="button" value="Baja" onclick="delete_pagos()"><img
                                src="https://img.icons8.com/nolan/50/delete.png" alt="Baja" /></button> <button
                        type="button" value="Modificar" onclick="update_pagos()"><img
                                src="https://img.icons8.com/nolan/50/edit--v1.png" alt="Modificar" /></button> <button
                        type="button" value="Cancelar" onclick="cancelar()"><img
                                src="https://img.icons8.com/officexs/50/000000/cancel-2.png" alt="Cancelar" /></button>
                <button type="button" value="Volver"
                        onclick="try{javascript:var mywindows = window.opener.document.getElementById('idpagos').value = window.document.getElementById('idpagos').value;}catch(error){};window.close()"><img
                                src="https://img.icons8.com/ios/50/000000/home.png" alt="Volver" /></button>
        </fieldset>
</form>
<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.24.0/moment.min.js"></script>
<script src="../../public/js/pagos_v.js"></script>